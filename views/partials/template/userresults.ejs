<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <h3 class="page-header"><a href="#col2viewusers" data-toggle="collapse">Your results</a></h3>
    <div id="col2viewusers" class="collapse in">
        <div class="responsive">
            <table class="table table-hover table-striped">
                <thead>
                <tr>
                    <th>Quiz No.</th>
                    <th>User</th>
                    <th>Score</th>
                    <th>Elapsed Time</th>
                </tr>
                </thead>
                <tbody>
                <% loadResults.forEach(function (quiz, name) { %>
                <tr>
                    <td><%= quiz.quizTakenQid %></td>
                    <td><%= quiz.quizTakenMail %></td>
                    <td><%= quiz.results %></td>
                    <td><%= quiz.elapTimes %></td>
                </tr>
                <% }) %>
                </tbody>
                
            </table>
        </div>
    </div>
</div>
